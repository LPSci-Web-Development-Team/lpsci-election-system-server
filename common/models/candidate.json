{
  "name": "candidate",
  "plural": "candidates",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "defaultFn": "uuid"
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "position": {
      "type": "string",
      "required": true
    },
    "imgUrl": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "parties": {
      "type": "belongsTo",
      "model": "party",
      "foreignKey": "partyId"
    },
    "voters": {
      "type": "hasMany",
      "model": "voter",
      "foreignKey": "candidateId",
      "through": "vote"
    }
  },
  "acls": [],
  "methods": {}
}
